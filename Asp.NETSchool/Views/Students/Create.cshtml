@model Student
@*pro nahravani studentu primo ze souboru, pozor name se musi shodovat s nazvem parametr, ktery predavam kontroleru*@
<form asp-controller="FileUpload" asp-action="Upload" enctype="multipart/form-data">
    <div class="m-2">
        <input class="me-2" type="file" name="file" />
        <input type="submit" value="Upload" />
    </div>
</form>
@*pro nahravani studentu primo ze souboru--KONEC*@
<form asp-action="Create">
    <div class="form-group mt-2">
        <label asp-for="@Model.FirstName"></label>
        <input type="text" asp-for="FirstName" class="form-control" />
    </div>
    <div class="form-group mt-2">
        <label asp-for="@Model.LastName"></label>
        <input type="text" asp-for="LastName" class="form-control" />
    </div>
    <div class="form-group mt-2">
        <label asp-for="@Model.DateOfBirth"></label>
        <input type="date" asp-for="DateOfBirth" class="form-control" />
    </div>
    <input class="btn btn-success mt-2" type="submit" value="Create" />
</form>
